-- PRE ---
V_INC_FIELD := (SELECT INCREMENTAL_FIELD FROM DATAFORCE.FOG.FILE_EXPORT_CONTROL WHERE CONTROL_ID = P_CONTROL_ID);
V_LAST_UPDATE_DATE := (SELECT MAX_INCREMENTAL_FIELD FROM DATAFORCE.FOG.FILE_EXPORT_CONTROL WHERE CONTROL_ID = P_CONTROL_ID); 

IF (V_INT_FIELD IS NOT NULL) THEN
    VS_TEMPSTM := ' WHERE ' || :V_INC_FIELD || ' > COALESCE(' || :V_LAST_UPDATE_DATE || ', ''1900-01-01'')' 
    VS_SQLSTM := CONCAT(VS_SQLSTM, VS_TEMPSTM);
END IF;

-- POST --

